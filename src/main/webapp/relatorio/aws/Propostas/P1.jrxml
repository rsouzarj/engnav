	<queryString>
		<![CDATA[select
documento.codigo, parceiro.nome,parceiro.pessoa_contato,
condicao_pagamento.nome condicao_pagamento,colaborador.nome as nome_c, colaborador.nome_assinatura,colaborador.documento,
parceiro_contato.nome nomeRequisitante,parceiro_contato.email emailRequisicao,
parceiro_contato.telefone telefoneRequisitante,parceiro_contato.info infoRequisitante,
documento.*
from
documento
left join parceiro on parceiro.seq_parceiro = documento.seq_parceiro
left join parceiro_contato on documento.seq_parceiro_contato = parceiro_contato.seq_parceiro_contato
left join colaborador on colaborador.seq_colaborador = documento.seq_ass_colaborador
left join condicao_pagamento on condicao_pagamento.SEQ_CONDICAO_PAGAMENTO = documento.seq_condicao_pagamento
where
documento.seq_documento = $P{pSeqDocumento}]]>
	</queryString>
