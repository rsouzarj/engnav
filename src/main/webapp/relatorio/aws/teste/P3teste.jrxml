	<subDataset name="Equipamentos" uuid="e31516e6-1e6a-4cb4-868e-870365859a1f">
		<parameter name="pSeqDocumentoEquipamento" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
documento_item_equipamento.seq_documento,
equipamento.nome
From
documento_item_equipamento
inner join equipamento on equipamento.seq_equipamento = documento_item_equipamento.seq_equipamento
where
documento_item_equipamento.seq_documento =  $P{pSeqDocumentoEquipamento}]]>
		</queryString>
		<field name="SEQ_DOCUMENTO" class="java.math.BigDecimal"/>
		<field name="NOME" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Escopo_Servico" uuid="51663f6a-6249-4831-9ece-83b014e3c2ff">
		<parameter name="pSeqDocumentoEscopoServico" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
  servico_escopo.nome
from
  documento_item_servico
inner join servico_escopo on servico_escopo.seq_servico_escopo = documento_item_servico.seq_servico_escopo
where
documento_item_servico.seq_documento = $P{pSeqDocumentoEscopoServico}]]>
		</queryString>
		<field name="NOME" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Tabela_Preco" uuid="17d176ae-cd64-41fd-be6a-ad8daeb1e242">
		<parameter name="pSeqDocumentoPreco" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
material.codigo,
material.referencia,
material.nome,
documento_item_material.vl_unitario,
documento_item_material.qtde,
documento_item_material.preco_total,
documento.moeda
 From
documento_item_material
inner join material on material.seq_material = documento_item_material.seq_material
inner join documento on documento.seq_documento = documento_item_material.seq_documento
where
documento_item_material.seq_documento = $P{pSeqDocumentoPreco}]]>
		</queryString>
		<field name="CODIGO" class="java.lang.String"/>
		<field name="REFERENCIA" class="java.lang.String"/>
		<field name="NOME" class="java.lang.String"/>
		<field name="VL_UNITARIO" class="java.math.BigDecimal"/>
		<field name="QTDE" class="java.math.BigDecimal"/>
		<field name="PRECO_TOTAL" class="java.math.BigDecimal"/>
		<field name="MOEDA" class="java.lang.String"/>
		<variable name="TotalGeral" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{PRECO_TOTAL}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="pSeqDocumento" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[261]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
documento.codigo, parceiro.nome,parceiro.pessoa_contato,
condicao_pagamento.nome condicao_pagamento,
lugar.nome lugar,colaborador.nome as nome_c, colaborador.nome_assinatura, colaborador.documento,
parceiro_contato.nome nomeRequisitante,parceiro_contato.email emailRequisitante,
parceiro_contato.telefone telefoneRequisitante,parceiro_contato.info infoRequisitante,
documento.*
from
documento
left join parceiro on parceiro.seq_parceiro = documento.seq_parceiro
left join parceiro_contato on parceiro_contato.seq_parceiro_contato = documento.seq_parceiro_contato
left join colaborador on colaborador.seq_colaborador = documento.seq_ass_colaborador
left join condicao_pagamento on condicao_pagamento.SEQ_CONDICAO_PAGAMENTO = documento.seq_condicao_pagamento
left join lugar on lugar.seq_lugar = documento.seq_lugar
where
seq_documento = $P{pSeqDocumento}]]>
	</queryString>
