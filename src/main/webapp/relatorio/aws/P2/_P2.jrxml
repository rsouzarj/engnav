	<subDataset name="Tabela_Preco" uuid="17d176ae-cd64-41fd-be6a-ad8daeb1e242">
		<parameter name="pSeqDocumentoPreco" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
material.codigo,
material.referencia,
material.nome,
documento_item_material.vl_unitario,
documento_item_material.qtde,
documento_item_material.preco_total
 From
documento_item_material
inner join material on material.seq_material = documento_item_material.seq_material
where
documento_item_material.seq_documento = $P{pSeqDocumentoPreco}]]>
		</queryString>
