				<textFieldExpression><![CDATA[$F{DESCRICAO}==null?"Nehum Necessário para este serviço!":$F{DESCRICAO}.getSubString( new Long(01), new Long($F{DESCRICAO}.length()).intValue())]]></textFieldExpression>
